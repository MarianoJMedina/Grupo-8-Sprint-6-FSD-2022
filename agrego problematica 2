<sql name="1">
CREATE VIEW Vista 
AS SELECT
customer_id,
branch_id,
customer_name,
customer_surname,
customer_DNI,
(2022-substr(dob, 1, 4)) AS Edad
FROM cliente
</sql>

<sql name="1.1">
SELECT customer_name, customer_surname, Edad, customer_DNI
FROM Vista
WHERE Edad &gt; 40
ORDER BY customer_DNI
</sql>

<sql name="1.2">
SELECT customer_name, customer_surname, Edad, customer_DNI
FROM Vista
WHERE customer_name = &quot;Anne&quot; OR customer_name = &quot;Tyler&quot;
ORDER BY Edad 
</sql>

