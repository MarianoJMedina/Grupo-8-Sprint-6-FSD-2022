<sql name="2.1">
SELECT customer_name, customer_surname, customer_DNI, branch_id, dob
FROM cliente
ORDER BY customer_id DESC
LIMIT 5
</sql>

<sql name="3">
UPDATE cliente
SET branch_id = &quot;10&quot;
WHERE customer_id BETWEEN 501 AND 505
</sql>

<sql name="4">
DELETE FROM cliente
WHERE customer_name = &quot;Noel&quot; AND customer_surname = &quot;David&quot;
</sql>

<sql name="5">
SELECT loan_type
FROM prestamo
ORDER BY loan_total DESC
LIMIT 1
</sql>

